{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "Basic react js codes",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
