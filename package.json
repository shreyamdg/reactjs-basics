    "react-dom": "^16.0.0",
    "react-router": "^4.2.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "prop-types": "^15.6.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
